<simulation>
  <archetypes>
    <spec>
      <lib>tut.agents</lib>
      <name>Storage</name>
    </spec>
    <spec>
      <lib>agents</lib>
      <name>NullInst</name>
    </spec>
    <spec>
      <lib>agents</lib>
      <name>NullRegion</name>
    </spec>
  </archetypes>
  <control>
    <duration>10</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>
  <facility>
    <config>
      <Storage>
        <incommod>fuel</incommod>
        <outcommod>stored_fuel</outcommod>
        <storage_time>1</storage_time>
        <throughput>10</throughput>
      </Storage>
    </config>
    <name>OneFacility</name>
  </facility>
  <region>
    <config>
      <NullRegion/>
    </config>
    <institution>
      <config>
        <NullInst/>
      </config>
      <initialfacilitylist>
        <entry>
          <number>1</number>
          <prototype>OneFacility</prototype>
        </entry>
      </initialfacilitylist>
      <name>OneInst</name>
    </institution>
    <name>OneRegion</name>
  </region>
</simulation>
